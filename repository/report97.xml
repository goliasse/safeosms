<?xml version='1.0' encoding='ISO-8859-1' standalone='no'?><!DOCTYPE REPORT SYSTEM 'PHPReport.dtd'><REPORT MARGINWIDTH='2' MARGINHEIGHT='2'>	<TITLE>Reporte Individual de Notas</TITLE>	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>	<SQL>SELECT numero_solicitud AS 'solicitud',		COUNT(numero_solicitud) AS		'repetidos'		FROM creditos_solicitud		GROUP BY numero_solicitud HAVING		COUNT(numero_solicitud) > 1</SQL>	<INTERFACE>mysql</INTERFACE>	<CONNECTION>localhost</CONNECTION>	<DATABASE>pakal</DATABASE>	<NO_DATA_MSG>NO EXISTEN DATOS</NO_DATA_MSG>	<CSS MEDIA='print'>../css/xml.css</CSS>	<CSS MEDIA='screen'>../css/xml.css</CSS>	<PAGE BORDER='0' SIZE='0' CELLSPACING='2' CELLPADDING='0'>		<HEADER><!-- CONTENIDO DEL ENCABEZADO DE PAGINA --><ROW><COL CELLCLASS='GHC' COLSPAN='3' TYPE='RAW_EXPRESSION'>getRawHeader(true)</COL></ROW>			<ROW>				<COL CELLCLASS='GHL' COLSPAN='3'>Reporte de Cuentas de Credito					Duplicadas</COL>			</ROW>			<ROW>				<COL CELLCLASS='GHL' WIDTH='100%' COLSPAN='3'>					<XHTML>						<hr />					</XHTML>				</COL>			</ROW>			<ROW>			</ROW>		</HEADER>		<FOOTER><!-- PIE DE PAGINA --><ROW><COL CELLCLASS='GHC' COLSPAN='3' TYPE='RAW_EXPRESSION'>getRawFooter(true)</COL></ROW>		</FOOTER>	</PAGE>	<GROUPS>		<GROUP NAME='MAIN' EXPRESSION=''>			<HEADER>				<ROW>					<COL CELLCLASS='GHC'>NUMERO DE SOLICITUD</COL>					<COL CELLCLASS='GHC'>NUM. VECES REPETIDO</COL>				</ROW>			</HEADER>			<FIELDS>				<ROW>					<COL TYPE='FIELD' CELLCLASS='FR'>solicitud</COL>					<COL TYPE='FIELD' CELLCLASS='FC'>repetidos</COL>				</ROW>			</FIELDS>			<FOOTER>				<ROW>					<COL TYPE='EXPRESSION' CELLCLASS='GHC' COLSPAN='3'>					</COL>				</ROW>				<ROW>					<COL CELLCLASS='GHC' WIDTH='100%' COLSPAN='3'>						<XHTML>							<hr />						</XHTML>					</COL>				</ROW>				<ROW>				</ROW>			</FOOTER>		</GROUP>	</GROUPS>	<DOCUMENT>		<FOOTER>			<ROW>			</ROW>		</FOOTER>	</DOCUMENT></REPORT>